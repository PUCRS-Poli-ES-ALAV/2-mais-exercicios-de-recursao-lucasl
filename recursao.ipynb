    "def Fatorial(a):\n",
    "    if (a == 0) or (a == 1):\n",
    "        return 1\n",
    "\n",
    "    return a * Fatorial (a-1)\n",
    "\n",
    "print(Fatorial(5))\n",
    "\n"
